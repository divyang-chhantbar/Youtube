{
    "manifest_version": 3,
    "name": "YouTube 4K Video Downloader",
    "version": "1.0",
    "description": "Download YouTube videos in 4K resolution.",
    "permissions": ["downloads", "tabs", "activeTab"],
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  